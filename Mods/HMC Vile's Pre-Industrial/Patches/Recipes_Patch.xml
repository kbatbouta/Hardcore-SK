<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Core SK</li>
	</mods>
	<match Class="PatchOperationSequence">
	<success>Always</success>
			<operations>

			<li Class="PatchOperationAdd">
				<xpath>Defs/RecipeDef[defName="MakeSulfur"]</xpath>
				<value>
					<recipeUsers>
						<li>TableStonecutter</li>
					</recipeUsers>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>Defs/RecipeDef[defName="MakePowder"]</xpath>
				<value>
					<recipeUsers>
						<li>TableStonecutter</li>
					</recipeUsers>
				</value>
			</li>
		
		
	
		</operations>
		</match>
    </Operation>	
</Patch>	