			<workerClass>NPS.NPS_BiomeWorker_BorealForest</workerClass>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/BiomeDef[defName = "ColdBog"]/workerClass</xpath>
		<value>
			<workerClass>NPS.NPS_BiomeWorker_ColdBog</workerClass>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/BiomeDef[defName = "Desert"]/workerClass</xpath>
		<value>
			<workerClass>NPS.NPS_BiomeWorker_Desert</workerClass>
